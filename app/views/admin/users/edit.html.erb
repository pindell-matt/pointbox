<h1>Edit <%= @user.username %></h1>

<%= form_for @user, url:{:controller=>'admin/users', :action=>'update'} do |f| %>
  <%= f.text_field :username, placeholder: "#{@user.username}" %>
  <%= f.text_field :password, placeholder: "#{@user.password}" %>

  <%= f.submit "Update #{@user.username}"%>
<% end %>
